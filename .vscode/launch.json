            "type": "java",
            "name": "CamsApplication",
            "request": "launch",
            "mainClass": "com.contoso.cams.CamsApplication",
            "projectName": "cams",
            "env": {
                "SPRING_PROFILES_ACTIVE": "local"
            }
        },
        {
            "type": "java",
            "name": "EmailProcessor",
            "request": "launch",
            "mainClass": "com.contoso.cams.emailprocessor.EmailProcessorApplication",
            "projectName": "email-processor",
            "env": {
                "AZURE_SERVICEBUS_NAMESPACE": "",
                "AZURE_SERVICEBUS_EMAIL_REQUEST_QUEUE_NAME": "",
                "AZURE_SERVICEBUS_EMAIL_RESPONSE_QUEUE_NAME": ""
            }
        }
    ]
